    "\n",
    "vector1_rot = vector1 * Ry\n",
    "vector2_rot = vector2 * Ry\n",
    "\n",
    "print ('Vector 1 rot:\\n', vector1_rot, '\\nVector 2 rot:\\n', vector2_rot)"
